<header class="Main-header">
  <h1>Cards</h1>
  <%= link_to("New Card", new_game_card_path) %>
</header>
<article class="Main-content">
  <% if @cards.empty? %>
    no cards.
  <% else %>
    <ul>
      <% @cards.each do |card| %>
        <li>
          <%= card.name %>
          <%= card.text %>
        </li>
      <% end %>
    </ul>
  <% end %>
<article>
