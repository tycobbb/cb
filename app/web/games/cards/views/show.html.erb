<header class="Main-header">
  <h1><%= @card.name %></h1>
  <menu class="Main-headerMenu">
    <%= link_to("Edit", edit_game_card_path(@card)) %>
    <%= link_to("Delete", @card, :method => :delete, :confirm => "Are you sure?") %>
  </menu>
</header>
<article class="ShowCard">
  <dl>
    <dt>Text</dt>
    <dd><%= @card.text %>
    <dt>Preview</dt>
    <dd><%= Cells::Card.render(@card) %></dd>
  </dl>
</article>
